<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
									namespace="July09v31.Core.Domain.Model"
									assembly="July09v31.Core">

	<class name="Attendee" table="Attendees" dynamic-update="true">
		<cache usage="read-write"/>
		<id name="Id" column="Id" type="Guid">
			<generator class="guid.comb"/>
		</id>

		<property name="FirstName" length="100"/>
		<property name="LastName" length="100"/>
		<property name="EmailAddress" length="100"/>
		<property name="Webpage" length="100"/>
		<property name="Status" type="July09v31.Infrastructure.EnumerationType`1[[July09v31.Core.Domain.Model.AttendanceStatus, July09v31.Core]],July09v31.Infrastructure"/>

	</class>
</hibernate-mapping>

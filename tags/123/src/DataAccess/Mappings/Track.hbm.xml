<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                  namespace="CodeCampServer.Model.Domain"
                  assembly="CodeCampServer.Model" default-access="field.camelcase-underscore"  >

	<class name="Track" table="Tracks">

		<id name="Id" type="Guid" column="Id">
			<generator class="guid.comb" />
		</id>

		<many-to-one name="Conference" column="ConferenceId" not-null="true"/>
		<property name="Name" length="200" not-null="true" unique="true"/>
	</class>

</hibernate-mapping>
